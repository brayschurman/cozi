{
    "manifest_version": 2,

    "name": "Cozi",
    "description": "Sonic Science",
    "version": "1.1",

    "browser_action": {
        "default_icon": "logo.png",
        "default_popup": "popup.html"
    },

    "background": {
        "scripts": ["js/jquery-3.1.1.min.js", "js/custom.js"],
        "persistent": false
    },

    "permissions": [
        "activeTab",
        "https://ajax.googleapis.com/"
    ]
}
